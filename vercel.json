{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "API_URL_GET_BECAS": "@api_url_get_becas",
    "API_URL_GET_PARAMETROS": "@api_url_get_parametros",
    "API_URL_POST_USUARIO": "@api_url_post_usuario",
    "API_URL_POST_LOGIN": "@api_url_post_login",
    "API_URL_GET_USER": "@api_url_get_user",
    "API_URL_UPDATE_USER": "@api_url_update_user"
  },
  "build": {
    "env": {
      "API_URL_GET_BECAS": "@api_url_get_becas",
      "API_URL_GET_PARAMETROS": "@api_url_get_parametros",
      "API_URL_POST_USUARIO": "@api_url_post_usuario",
      "API_URL_POST_LOGIN": "@api_url_post_login",
      "API_URL_GET_USER": "@api_url_get_user",
      "API_URL_UPDATE_USER": "@api_url_update_user"
    },
    "command": "node generate-config.js"
  }
}
